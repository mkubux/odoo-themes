<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_loengo/static/scss/style.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_loengo/static/scss/theme_danger.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_loengo/static/scss/theme_blue.scss"/>
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script src="/theme_loengo/static/js/scripts.js"></script>
        </xpath>
    </template>
</odoo>